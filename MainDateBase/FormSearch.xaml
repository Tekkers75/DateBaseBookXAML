<Window x:Class="MainDateBase.FormSearch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainDateBase"
        mc:Ignorable="d"
        Title="FormSearch" Height="450" Width="800">
    <Grid>
        <Button x:Name="Button_Search" Content="Найти" HorizontalAlignment="Left" Margin="320,266,0,0" VerticalAlignment="Top" Click="Button_Search_Click"/>
        <Button x:Name="Button_Close" Content="Закрыть" HorizontalAlignment="Left" Margin="400,266,0,0" VerticalAlignment="Top"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="256,103,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox_Search" HorizontalAlignment="Left" Margin="320,179,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
